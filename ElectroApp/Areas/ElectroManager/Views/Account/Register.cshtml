
@{
    Layout = null;
}
@inject ElectroApp.Services.LayoutServices layoutService
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css"
          integrity="sha512-9usAa10IRO0HhonpyAIVpjrylPvoDwiPUiKdWk5t3PyolY1cOd4DSE0Ga+ri4AuTroPR5aQvXU9xC6qOPnzFeg=="
          crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Old+Standard+TT:ital,wght@0,400;0,700;1,400&family=Open+Sans:wght@300;400;500;600;700;800&family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;1,100;1,300;1,400;1,500&family=Rubik:ital,wght@0,300;0,400;0,500;1,300;1,400&display=swap"
          rel="stylesheet">
    <!-- owl caorusel -->
    <link rel="stylesheet" href="~/plugins/OwlCarousel2-2.3.4/docs/assets/owlcarousel/assets/owl.carousel.min.css">
    <link rel="stylesheet" href="~/plugins/OwlCarousel2-2.3.4/docs/assets/owlcarousel/assets/owl.theme.default.min.css">
    <!-- --------------------- -->
    <link rel="stylesheet" href="~/plugins/ui-main/dist/fancybox.css" />
    <link rel="stylesheet" href="~/plugins/star-rating-svg-master/src/css/star-rating-svg.css">
    <link rel="stylesheet" href="~/assets/sass/main.css" />
    <title>Electro</title>
</head>
<body>
    <div id="loginPage" style="height:100vh !important;display:flex;align-items:center !important;">
        <section id="account" style="width:100%;">
            <div class="container">
                <div class="row">
                    <div class="account">
                        <div class="loginwrapper">
                            <div class="login">
                                <div class="heading">
                                    <h4 class="nameofdiv">Login for Admin</h4>
                                </div>
                                <div class="content">
                                    <div class="returnToStore">
                                        <p class="description">Welcome back! Sign in yo your account</p>
                                        <a href="">Return to Store</a>
                                    </div>

                                    <form action="">
                                        <label for="">Email Address *</label>
                                        <div class="inputwrapper">
                                            <input type="text" id="email">
                                        </div>
                                        <label for="">Password *</label>
                                        <div class="inputwrapper">
                                            <input type="text" id="password">
                                        </div>
                                        <div class="bottomlogin">

                                            <div style="display:flex;align-items:center;">
                                                <input style="margin-right:5px;width:16px;height:16px;" type="checkbox" />
                                                <label style="margin-bottom:0px;">Remember me</label>
                                            </div>

                                            <a href="#" class="forgonpass">Forgotten Password ?</a>
                                        </div>
                                        <input class="inpSubmit" type="submit" value="Login">
                                    </form>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>
    @*jquery*@
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js"
            integrity="sha512-894YE6QWD5I59HgZOGReFYm4dnWc1Qt5NtvYSaNcOP+u1T9qYdvdihz0PPSiiqn/+/3e7Jo4EaG7TubfWGUrMQ=="
            crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    @*owl carousel*@
    <script src="~/plugins/OwlCarousel2-2.3.4/docs/assets/owlcarousel/owl.carousel.min.js"></script>
    @*----------------*@
    @*------progressbar----*@
    <script src="~/assets/JS/skill.bars.jquery.js"></script>
    @*-----------------*@
    <script src="~/assets/js/script.js"></script>
</body>

</html>