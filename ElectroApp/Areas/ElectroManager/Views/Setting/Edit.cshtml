@model Setting
@{ ViewData["Title"] = "Edit"; }
<div class="container">
    <h2 class="mt-3">Edit Settings</h2>
    <form class="mt-3" method="post">
        <h5 class="mt-1 mb-1">Header</h5>
        <div class="mb-3">
            <label asp-for="@Model.Logo" class="form-label">Logo</label>
            <input asp-for="@Model.Logo" class="form-control">
            <span class="text text-danger" asp-validation-for="@Model.Logo"></span>
        </div>
        <div class="mb-3">
            <label asp-for="@Model.hContact" class="form-label">Contact</label>
            <input asp-for="@Model.hContact" class="form-control">
            <span class="text text-danger" asp-validation-for="@Model.hContact"></span>
        </div>
        <div class="mb-3">
            <label asp-for="@Model.hEmail" class="form-label">Email</label>
            <input asp-for="@Model.hEmail" class="form-control">
            <span class="text text-danger" asp-validation-for="@Model.hEmail"></span>
        </div>
        <button type="submit" class="btn btn-primary">Edit</button>
    </form>
</div>
@section Scripts{
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.3/jquery.validate.min.js" integrity="sha512-37T7leoNS06R80c8Ulq7cdCDU5MNQBwlYoy1TX/WUsLFC2eYNqtKlV0QjH7r8JpG/S0GUMZwebnVFLPd6SU5yg==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validation-unobtrusive/3.2.12/jquery.validate.unobtrusive.min.js" integrity="sha512-o6XqxgrUsKmchwy9G5VRNWSSxTS4Urr4loO6/0hYdpWmFUfHqGzawGxeQGMDqYzxjY9sbktPbNlkIQJWagVZQg==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
}