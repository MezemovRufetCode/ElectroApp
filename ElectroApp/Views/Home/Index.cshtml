                        <div class="special-productByCat col-lg-4 col-md-6 col-sm-12 col-12">
                            <p class="announceSp">Special <br> Offer</p>
                            <p class="discountSp">Save <br><span class="discountPercent">@product.Campaign.DiscountPercent%</span></p>
                            <div class="productImage">
                                <div class="specialOfferImage">
                                    @foreach (var item in product.ProductImages)
                                    {
                                        if (item.IsMain)
                                        {
                                            <a href="#">
                                                <img src="~/assets/images/featuredProducts/@item.Image" alt="">
                                            </a>
                                        }
                                    }
                                </div>
                            </div>
