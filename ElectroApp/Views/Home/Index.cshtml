                                <div class="productName">
                                    <a href="#">
                                        <p class="spProductName">@product.Name</p>
                                    </a>
                                </div>
                                <div class="productPrice">
                                    @if (product.CampaignId == null)
                                    {
                                        <p class="spProductCurrentPrice">$@(Math.Floor((product.Price)*100)/100)</p>
                                    }
                                    else
                                    {
                                        <p class="spProductCurrentPrice">$@(Math.Floor((product.Price*(100-product.Campaign.DiscountPercent)/100)*100)/100)<span class="oldPrice">$@product.Price</span></p>
                                    }
                                </div>
                                <div class="stockCondition">
                                    <div class="stockCount">
                                        <p class="sold">Already Sold: <span>15</span></p>
                                        <p class="avaliable">Avaliable: <span>@product.AvaliableCount</span></p>
                                    </div>
                                </div>
